generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Int
  stock       Int
  category    String
  rating      Float?
  description String?
  sku         String
  image       String?
  images      String[]
  createdAt   DateTime @default(now())
}

